<div class="col-md-6">
  <h1>Team: $gotTeam.getName()</h1>
  #if($gotTeam.getMembers())
    <h2>Members</h2>
    #foreach($teamMember in $gotTeam.getMembers())
      <h3>Name: $teamMember.getName()</h3>
      <h3>language: $teamMember.getLanguage()</h3></h3>
    #end
  #else
    <h2>No Members in Team</h2>
  #end
</div>
